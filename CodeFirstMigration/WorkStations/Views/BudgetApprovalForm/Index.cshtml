
@{
    ViewBag.Title = "Budget Approval Form";
}
@section styles{


    <style>
        .ui-datepicker {
            z-index: 9999 !important;
        }

        div.ui-datepicker {
            font-size: 10px;
        }

        #applicant-details-table {
            border-collapse: collapse;
            border: 1px solid #ddd;
        }

            #applicant-details-table th, #applicant-details-table td {
                border: 1px solid #ddd;
                padding: 8px;
                text-align: left;
            }

            #applicant-details-table th {
                background-color: #f2f2f2;
            }

            #applicant-details-table center {
                display: block;
            }

        th {
            text-align: center;
        }

        .disabled {
            pointer-events: none;
            opacity: 0.5;
        }

        #ls_add_trf_modal .modal-content,
        #ls_add_trf_modal .modal-content * {
            font-size: 12px;
        }

        #ls_edit_trf_modal .modal-content {
            font-size: 13px;
            max-height: 85vh;
            overflow-y: auto;
        }

        #ls_edit_trf_modal .modal-body {
            padding-bottom: 0;
        }

            #ls_edit_trf_modal .modal-body .row {
                margin-bottom: 10px;
            }

        .scrollable-content {
            max-height: 70vh;
            overflow-y: auto;
        }

        .hidden {
            display: none;
        }

    </style>




}
<div class="page-heading">
    <section class="section">
        <div class="section-body">
            <div class="row">
                <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xs-12">
                    <div class="card  shadow">
                        <div class="card-header" style="background-color:#e0e0e0">
                            <div class="row">
                                <div class="col-12 col-md-6 order-md-1 order-last">
                                    <h3>
                                        Budget Approval Form
                                    </h3>
                                </div>
                                <div class="col-12 col-md-6 order-md-2 order-first">
                                    <div class="breadcrumb-header float-start float-lg-end">
                                        <a class="btn btn-primary rounded-pill" data-toggle="modal" id="ls_add_trf_btn">Add TRF</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card-body">

                            <form class="form-inline" method="get" id="gatepass_filter_form" name="gatepass_filter_form">

                                <div class="row mt-3">
                                    <div class="col-lg-3 mb-1">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text">Limit</span>
                                            <select id="filter_limit" name="filter_limit" class="form-control form-select">
                                                <option value="10" selected>10</option>
                                                <option value="25">25</option>
                                                <option value="50">50</option>
                                                <option value="100">100</option>
                                                <option value="250">250</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3 mb-1">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text">
                                                Budget Approval NO
                                            </span>
                                            <input id="filter_trf_no" name="filter_trf_no" class="form-control" />
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Employee ID</span>
                                            <input id="ls_rp_EmpInfo_filter_Id" name="ls_rp_EmpInfo_filter_Id" class="form-control" />
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Company</span>
                                            <select class="form-control form-control form-select" id="ls_filter_rp_EmpInfo_Company" name="ls_filter_rp_EmpInfo_Company">
                                                <option value="">Select a Company</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Department</span>
                                            <select class="form-control form-control form-select" id="ls_filter_rp_EmpInfo_department" name="ls_filter_rp_EmpInfo_department">
                                                <option value="">Select a Department</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Designation</span>
                                            <select class="form-control form-control form-select" id="ls_filter_rp_EmpInfo_designation" name="ls_filter_rp_EmpInfo_designation">
                                                <option value="">Select a Designation</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="btn-group">
                                            <button type="button" class="btn btn-soft-blue" id="ls_filter_btn" name="ls_filter_btn">Filter</button>
                                        </div>
                                    </div>
                                </div>
                            </form>

                            <div class="table-responsive mt-2">
                                <table class="table table-striped table-hover" id="ls_trf_table">
                                    <thead>
                                        <tr>
                                            <th>SL NO</th>
                                            <th>
                                                Budget Approval NO
                                            </th>
                                            <th>Employee ID NO</th>
                                            <th>Name</th>
                                            <th>Contact NO</th>
                                            <th>Company</th>
                                            <th>Department</th>
                                            <th>Designation</th>
                                            <th>Total Cost Required</th>
                                            <th>Total Advance Required</th>
                                            <th>Remarks</th>
                                            <th>Approval Status (Supervisor)</th>
                                            <th>Approved By (Supervisor)</th>
                                            <th>Comments (Supervisor; If rejected)</th>
                                            <th>Approval Status (Management)</th>
                                            <th>Approved By (Management)</th>
                                            <th>Comments (Management; If rejected)</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody id="ls_trf_table_tbody"></tbody>

                                </table>
                            </div>



                            <div class="row">
                                @*table info*@
                                <div class="col-md-4">
                                    <span class="badge bg-primary" id="ls_trf_table_info"></span>
                                </div>
                                @*table spinner*@

                                <div class="col-md-4">
                                    <div class="text-center" id="spinner">
                                        <img src="~/Content/assets/vendors/svg-loaders/rings.svg" class="" style="width: 3rem" alt="audio"><span style="font-weight: 700">Loading...</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>



    <!--trf add modal -->
    <div class="modal fade bd-example-modal" id="ls_add_trf_modal">
        <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-header bg-primary text-white">
                    <h5 class="modal-title">
                        Budget Approval Form
                    </h5>
                    <button type="button" class="close text-white" data-bs-dismiss="modal" aria-label="Close">
                        <i data-feather="x"></i>
                    </button>
                </div>
                <form id="ls_add_trf_modal_form" name="ls_add_trf_modal_form" class="needs-validation" novalidate>
                    <div class="modal-body">
                        <div class="scrollable-content">
                            <div class="row">
                                <!-- Currency Type -->
                                <div class="col-md-6 mb-3">
                                    <label for="ls_add_ticketreq_currtype" class="form-label required">Currency Type</label>
                                    <select id="ls_add_ticketreq_currtype" name="ls_add_ticketreq_currtype" class="form-select" required>
                                        <option value="-1" disabled selected>Select Currency Type...</option>
                                    </select>
                                    <div class="invalid-feedback">
                                        Please select a Currency Type.
                                    </div>
                                </div>

                                <!-- Remarks -->
                                <div class="col-md-6 mb-3">
                                    <label for="ls_NeedleReq_add_remarks" class="form-label">Remarks</label>
                                    <input id="ls_NeedleReq_add_remarks" name="ls_NeedleReq_add_remarks" class="form-control" />
                                </div>
                            </div>


                            <!-- Team Member Details -->
                            <div class="card border-primary mb-3">
                                <div class="card-header">
                                    <h4 class="card-title">Visa, Ticket and Hotel Reservation Requisitions Details</h4>
                                    <small>Click [+] to add a new row. Double-click any field to open a selection modal for requisition numbers. Note: TRF cannot proceed without a valid selection.</small>                                    
                                </div>
                                <div class="card-body p-0">
                                    <div class="table-responsive">
                                        <table class="table table-striped table-hover" id="ls_team_table">
                                            <thead class="table-primary">
                                                <tr>
                                                    <th>Visa Requisition No.</th>
                                                    <th>Ticket Requisition No.</th>
                                                    <th>Hotel Requisition No.</th>
                                                    <th>Action</th>
                                                </tr>
                                            </thead>
                                            <tbody id="ls_team_table_tbody">
                                                <tr id="ginnuepig" class="text-center add-item">
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                    <td>
                                                        <button type="button" class="btn btn-primary" id="add_item_btn" aria-label="Add Item" style="font-family: auto;">
                                                            <i class="bi bi-plus-square"></i>
                                                        </button>
                                                    </td>
                                                </tr>
                                            </tbody>


                                        </table>
                                    </div>
                                </div>
                            </div>

                            <div class="alert alert-info" role="alert">
                                <strong>Notice:</strong> The food allowance of $100 USD has been established. If you are invoicing in a different currency, please convert and submit the equivalent amount to $100 USD.
                            </div>


                            <!-- Estimated Travel Cost -->
                            <div class="card border-primary mb-3">
                                <div class="card-header">
                                    <h4 class="card-title">Estimated Travel Cost</h4>
                                </div>
                                <div class="card-body p-0">
                                    <div class="table-responsive">
                                        <table class="table table-striped table-hover">
                                            <thead class="table-primary">
                                                <tr>
                                                    <th>Expenses</th>
                                                    <th>Days</th>
                                                    <th>Cost per Day/Cost</th>
                                                    <th>Total</th>
                                                </tr>
                                            </thead>
                                            <tbody id="ord_order_table_tbody">
                                                <tr class="text-center add-item">
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                            <!-- Total Cost and Travel Advance -->
                            <div class="row">
                                <div class="col-md-6 mb-3">
                                    <label for="totalCostRequired" class="form-label">Total Cost Required <span class="text-danger">*</span></label>
                                    <input id="totalCostRequired" name="totalCostRequired" class="form-control" required readonly />
                                </div>

                                <div class="col-md-6 mb-3">
                                    <label for="travelAdvanceApproved" class="form-label">Travel Advance Required <span class="text-danger">*</span></label>
                                    <input id="travelAdvanceApproved" name="travelAdvanceRequired" class="form-control" required />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary rounded-pill" data-bs-dismiss="modal">
                            <i class="bx bx-x d-block d-sm-none"></i><span class="d-none d-sm-block">Close</span>
                        </button>
                        <button type="submit" class="btn btn-primary rounded-pill ml-1">
                            <i class="bx bx-check d-block d-sm-none"></i>
                            <span class="d-none d-sm-block">Save</span>
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>


@{
    ViewBag.Title = "Hotel Reservation ";
}
@section styles{

    <link href="~/Content/assets/vendors/timepicker/timepicker.css" rel="stylesheet" />
    <link href="~/_Assets/js/TMS/all.min.css" rel="stylesheet" />




    <style>
        .ui-datepicker {
            z-index: 9999 !important;
        }

        #applicant-details-table {
            border-collapse: collapse;
            border: 1px solid #ddd;
        }

            #applicant-details-table th, #applicant-details-table td {
                border: 1px solid #ddd;
                padding: 8px;
                text-align: left;
            }

            #applicant-details-table th {
                background-color: #f2f2f2;
            }

            #applicant-details-table center {
                display: block;
            }

        th {
            text-align: center;
        }

        .clockpicker-popover {
            z-index: 9999;
        }

        .disabled {
            pointer-events: none;
            opacity: 0.5;
        }
    </style>




}
<div class="page-heading">

                                <div class="col-12 col-md-6 order-md-1 order-last">
                                    <h3>Hotel Reservation</h3>
                                </div>
                                <div class="col-12 col-md-6 order-md-2 order-first">
                                    <div class="breadcrumb-header float-start float-lg-end">
                                        <a class="btn btn-primary rounded-pill" data-toggle="modal" id="ls_add_hotelresv_btn">Add</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card-body">

                            <form class="form-inline" method="get" id="gatepass_filter_form" name="gatepass_filter_form">

                                <div class="row mt-3">
                                    <div class="col-lg-3 mb-1">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text">Limit</span>
                                            <select id="filter_limit" name="filter_limit" class="form-control form-select">
                                                <option value="10" selected>10</option>
                                                <option value="25">25</option>
                                                <option value="50">50</option>
                                                <option value="100">100</option>
                                                <option value="250">250</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3 mb-1">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text">Hotel Reservation No.</span>
                                            <input id="filter_hotelresv_no" name="filter_hotelresv_no" class="form-control" />
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Employee ID</span>
                                            <input id="ls_rp_EmpInfo_filter_Id" name="ls_rp_EmpInfo_filter_Id" class="form-control" />
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Company</span>
                                            <select class="form-control form-control form-select" id="ls_filter_rp_EmpInfo_Company" name="ls_filter_rp_EmpInfo_Company">
                                                <option value="">Select a Company</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Department</span>
                                            <select class="form-control form-control form-select" id="ls_filter_rp_EmpInfo_department" name="ls_filter_rp_EmpInfo_department">
                                                <option value="">Select a Department</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Designation</span>
                                            <select class="form-control form-control form-select" id="ls_filter_rp_EmpInfo_designation" name="ls_filter_rp_EmpInfo_designation">
                                                <option value="">Select a Designation</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3 mb-1">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text">Status</span>
                                            <select id="filter_gatepass_status" name="filter_gatepass_status" class="form-control form-select">
                                                <option value="-1" selected>All...</option>
                                                <option value="0">Inactive</option>
                                                <option value="1">Active</option>
                                            </select>
                                        </div>

                                    </div>

                                    <div class="col-lg-3">
                                        <div class="btn-group">
                                            <button type="button" class="btn btn-soft-blue" id="ls_filter_btn" name="ls_filter_btn">Filter</button>
                                        </div>
                                    </div>
                                </div>
                            </form>

                            <table id="applicant-details-table" style="width: 100%; margin: 0 auto; margin-top: 10px;">
                                <tr>
                                    <th colspan="12"><center>Applicant Details</center></th>
                                </tr>
                                <tr>

                                    <td>Employee Name </td>
                                    <td id="applicant_name"></td>
                                    <td>Employee ID </td>
                                    <td id="applicant_employee_id"></td>
                                    <td>Company</td>
                                    <td id="applicant_company"></td>
                                    <td>Department</td>
                                    <td id="applicant_department"></td>
                                    <td>Designation</td>
                                    <td id="applicant_designation"></td>
                                </tr>
                            </table>


                            <br /><br />

                            <div class="table-responsive mt-2">
                                <table class="table table-striped table-hover" id="ls_hotelresv_table">
                                    <thead>
                                        <tr>
                                            <th>SL NO</th>
                                            <th>Hotel Requisition No.</th>
                                            <th>Employee ID NO</th>
                                            <th>Name</th>
                                            <th>Contact NO</th>
                                            <th>Company</th>
                                            <th>Department</th>
                                            <th>Designation</th>
                                            <th>Country</th>
                                            <th>City</th>
                                            <th>Check In Date</th>
                                            <th>Check Out Date</th>
                                            <th>Other Special Requests</th>
                                            <th>Meeting/Event Date</th>
                                            @*<th>Approval Status</th>
                                            <th>Approved/Rejected By</th>
                                            <th>Comments (If rejected)</th>*@
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody id="ls_hotelresv_table_tbody"></tbody>

                                </table>
                            </div>



                            <div class="row">
                                @*table info*@
                                <div class="col-md-4">
                                    <span class="badge bg-primary" id="ls_hotelresv_table_info"></span>
                                </div>
                                @*table spinner*@


                                        <select id="ls_add_ticketreq_airlines" name="ls_add_ticketreq_airlines" class="form-control form-select">
                                            <option value="-1">Select Airlines...</option>
                                        </select>
                                    </div>
                                    <div class="invalid-feedback">
                                        Please select an Airlines.
                                    </div>
                                </div>



                            </div>
                            @*First Column Ends*@



                            @*Second Column Starts*@

                            <div class="col-sm-6">

                                @*Frequent Flayer No*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">
                                            Frequent Flayer No
                                        </span>
                                        <input id="ls_add_ticketreq_flayerNo" name="ls_add_ticketreq_flayerNo" class="form-control" />
                                    </div>
                                </div>


                                @*Preferred Flight Number (if any)*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">
                                            Preferred Flight Number  (if any)
                                        </span>
                                        <input id="ls_add_ticketreq_flightno" name="ls_add_ticketreq_flightno" class="form-control" />
                                    </div>
                                </div>


                                @*Seat Preference*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">Seat Preference</span>
                                        <select id="ls_add_ticketreq_seat" name="ls_add_ticketreq_seat" class="form-control form-select">
                                            <option value="-1">Select Seat Preference...</option>
                                            <option value="1">Aisle</option>
                                            <option value="2">Window</option>
                                        </select>
                                    </div>
                                    <div class="invalid-feedback">
                                        Please select Seat Preference.
                                    </div>
                                </div>


                                @*Meal Preference (if any)*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">
                                            Meal Preference (if any)
                                        </span>
                                        <input id="ls_add_ticketreq_meal" name="ls_add_ticketreq_meal" class="form-control" />
                                    </div>
                                </div>

                                @*Special Requests/Requirements*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">
                                            Special Requests/Requirements:
                                        </span>
                                        <input id="ls_add_ticketreq_spreq" name="ls_add_ticketreq_spreq" class="form-control" />
                                    </div>
                                </div>

                                @*Remarks*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">
                                            Remarks
                                        </span>
                                        <input id="ls_add_ticketreq_remarks" name="ls_add_ticketreq_remarks" class="form-control" />
                                    </div>
                                </div>
                            </div>
                            @*Second Column Ends*@
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-light-secondary rounded-pill" data-bs-dismiss="modal">
                            <i class="bx bx-x d-block d-sm-none"></i><span class="d-none d-sm-block">Close</span>
                        </button>
                        <button type="submit" class="btn btn-primary rounded-pill ml-1">
                            <i class="bx bx-check d-block d-sm-none"></i>
                            <span class="d-none d-sm-block">Save</span>
                        </button>
                    </div>
                </form>

            </div>
        </div>
    </div>

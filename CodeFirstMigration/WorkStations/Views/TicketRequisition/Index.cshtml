
@{
    ViewBag.Title = "Ticket Requisition";
}
@section styles{

    <link href="~/Content/assets/vendors/timepicker/timepicker.css" rel="stylesheet" />
    @*<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" />*@
    <link href="~/_Assets/js/TMS/all.min.css" rel="stylesheet" />



    <style>
        .ui-datepicker {
            z-index: 9999 !important;
        }

/*        div.ui-datepicker {
            font-size: 10px;
        }*/

        #applicant-details-table {
            border-collapse: collapse;
            border: 1px solid #ddd;
        }

            #applicant-details-table th, #applicant-details-table td {
                border: 1px solid #ddd;
                padding: 8px;
                text-align: left;
            }

            #applicant-details-table th {
                background-color: #f2f2f2;
            }

            #applicant-details-table center {
                display: block;
            }

        th {
            text-align: center;
        }

        .clockpicker-popover {
            z-index: 9999;
        }

        .disabled {
            pointer-events: none;
            opacity: 0.5;
        }
    </style>




}
<div class="page-heading">
    <section class="section">
        <div class="section-body">
            <div class="row">
                <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xs-12">
                    <div class="card  shadow">
                        <div class="card-header" style="background-color:#e0e0e0">
                            <div class="row">
                                <div class="col-12 col-md-6 order-md-1 order-last">
                                    <h3>Ticket Requisition</h3>
                                </div>
                                <div class="col-12 col-md-6 order-md-2 order-first">
                                    <div class="breadcrumb-header float-start float-lg-end">
                                        <a class="btn btn-primary rounded-pill" data-toggle="modal" id="ls_add_ticketreq_btn">Add Ticket Requisition</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card-body">

                            <form class="form-inline" method="get" id="gatepass_filter_form" name="gatepass_filter_form">

                                <div class="row mt-3">
                                    <div class="col-lg-3 mb-1">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text">Limit</span>
                                            <select id="filter_limit" name="filter_limit" class="form-control form-select">
                                                <option value="10" selected>10</option>
                                                <option value="25">25</option>
                                                <option value="50">50</option>
                                                <option value="100">100</option>
                                                <option value="250">250</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3 mb-1">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text">Ticket Requisition No.</span>
                                            <input id="filter_ticketreq_no" name="filter_ticketreq_no" class="form-control" />
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Employee ID</span>
                                            <input id="ls_rp_EmpInfo_filter_Id" name="ls_rp_EmpInfo_filter_Id" class="form-control" />
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Company</span>
                                            <select class="form-control form-control form-select" id="ls_filter_rp_EmpInfo_Company" name="ls_filter_rp_EmpInfo_Company">
                                                <option value="">Select a Company</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Department</span>
                                            <select class="form-control form-control form-select" id="ls_filter_rp_EmpInfo_department" name="ls_filter_rp_EmpInfo_department">
                                                <option value="">Select a Department</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Designation</span>
                                            <select class="form-control form-control form-select" id="ls_filter_rp_EmpInfo_designation" name="ls_filter_rp_EmpInfo_designation">
                                                <option value="">Select a Designation</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3 mb-1">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text">Status</span>
                                            <select id="filter_gatepass_status" name="filter_gatepass_status" class="form-control form-select">
                                                <option value="-1" selected>All...</option>
                                                <option value="0">Inactive</option>
                                                <option value="1">Active</option>
                                            </select>
                                        </div>

                                    </div>

                                    <div class="col-lg-3">
                                        <div class="btn-group">
                                            <button type="button" class="btn btn-soft-blue" id="ls_filter_btn" name="ls_filter_btn">Filter</button>
                                        </div>
                                    </div>
                                </div>
                            </form>

                            <table id="applicant-details-table" style="width: 100%; margin: 0 auto; margin-top: 10px;">
                                <tr>
                                    <th colspan="12"><center>User Details</center></th>
                                </tr>
                                <tr>
                                    <td>Employee Name </td>
                                    <td id="applicant_name"></td>
                                    <td>Employee ID </td>
                                    <td id="applicant_employee_id"></td>
                                    <td>Company</td>
                                    <td id="applicant_company"></td>
                                    <td>Department</td>
                                    <td id="applicant_department"></td>
                                    <td>Designation</td>
                                    <td id="applicant_designation"></td>
                                </tr>
                            </table>


                            <br /><br />

                            <div class="table-responsive mt-2">
                                <table class="table table-striped table-hover" id="ls_ticketreq_table">
                                    <thead>
                                        <tr>
                                            <th>SL NO</th>
                                            <th>Ticket Requisition No.</th>
                                            <th>Employee ID NO</th>
                                            <th>Name</th>
                                            <th>Contact NO</th>
                                            <th>Company</th>
                                            <th>Department</th>
                                            <th>Designation</th>
                                            <th>Destination</th>
                                            <th>Departure Date</th>
                                            <th>Return Date</th>
                                            <th>Other Special Requests</th>
                                            <th>Remarks</th>
                                            @*<th>Approval Status</th>
                                            <th>Approved/Rejected By</th>
                                            <th>Comment (If rejected)</th>*@
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody id="ls_ticketreq_table_tbody"></tbody>

                                </table>
                            </div>



                            <div class="row">
                                @*table info*@
                                <div class="col-md-4">
                                    <span class="badge bg-primary" id="ls_ticketreq_table_info"></span>
                                </div>
                                @*table spinner*@

                                <div class="col-md-4">
                                    <div class="text-center" id="spinner">
                                        <img src="~/Content/assets/vendors/svg-loaders/rings.svg" class="" style="width: 3rem" alt="audio"><span style="font-weight: 700">Loading...</span>
                                    </div>
                                </div>

                                @*table pagination*@
                                <div class="col-md-4">
                                    <nav aria-label="Page navigation example">
                                        <ul class="pagination pagination-primary  justify-content-end">
                                            <li class="page-item">
                                                <a class="page-link" id="ls_ticketreq_previous_list_click" title="Previous">
                                                    <span aria-hidden="true"><i class="bi bi-chevron-left"></i></span>
                                                </a>
                                            </li>
                                            <li class="page-item active">
                                                <a class="page-link" id="ls_ticketreq_pagination_page_no" title="Click here for jump to page">1</a>
                                            </li>
                                            <li class="page-item">
                                                <input id="ls_ticketreq_pagination_page_no_input" class="page-link display-none" style="width:50px" />
                                            </li>
                                            <li class="page-item">
                                                <a class="page-link" id="ls_ticketreq_next_list_click" title="Next">
                                                    <span aria-hidden="true"><i class="bi bi-chevron-right"></i></span>
                                                </a>
                                            </li>
                                        </ul>
                                    </nav>
                                </div>

                            </div>


                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!--ticketreq add modal -->

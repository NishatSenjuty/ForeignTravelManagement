                                </div>
                            </div>


                            @*Flight Files*@
                            <div class="form-group">
                                <div class="input-group">
                                    <label for="ls_add_finalSettle_attachment_Flight" class="input-group-text">
                                        <b>Flight</b> &nbsp; Supporting Documents Upload (if applicable)
                                    </label>
                                    <input type="file" id="ls_add_finalSettle_attachment_Flight" name="ls_add_finalSettle_attachment_Flight" class="form-control" multiple />
                                </div>
                                <small class="text-muted">Accepted file types: .jpeg, .pdf, .doc etc</small>
                                <div class="mt-2 uploaded-design-flight"></div>
                            </div>

                            @*Hotel Files*@
                            <div class="form-group">
                                <div class="input-group">
                                    <label for="ls_add_finalSettle_attachment_hotel" class="input-group-text">
                                        <b>Hotel</b> &nbsp; Supporting Documents Upload (if applicable)
                                    </label>
                                    <input type="file" id="ls_add_finalSettle_attachment_hotel" name="ls_add_finalSettle_attachment_hotel" class="form-control" multiple />
                                </div>
                                <small class="text-muted">Accepted file types: .jpeg, .pdf, .doc etc</small>
                                <div class="mt-2 uploaded-design-hotel"></div>
                            </div>

                            @*Food Files*@
                            <div class="form-group">
                                <div class="input-group">
                                    <label for="ls_add_visareq_invitation_letter" class="input-group-text">
                                        <b>Food & Allowance</b> &nbsp; Supporting Documents Upload (if applicable)
                                    </label>
                                    <input type="file" id="ls_add_finalSettle_attachment_Food" name="ls_add_finalSettle_attachment_Food" class="form-control" multiple />
                                </div>
                                <small class="text-muted">Accepted file types: .jpeg, .pdf, .doc etc</small>
                                <div class="mt-2 uploaded-design-food"></div>
                            </div>

                            @*Transportation Files*@
                            <div class="form-group">
                                <div class="input-group">
                                    <label for="ls_add_finalSettle_attachment_Transportation" class="input-group-text">
                                        <b>Transportation</b> &nbsp; Supporting Documents Upload (if applicable)
                                    </label>
                                    <input type="file" id="ls_add_finalSettle_attachment_Transportation" name="ls_add_finalSettle_attachment_Transportation" class="form-control" multiple />
                                </div>
                                <small class="text-muted">Accepted file types: .jpeg, .pdf, .doc etc</small>
                                <div class="mt-2 uploaded-design-Transportation"></div>
                            </div>

                            @*Others Files*@
                            <div class="form-group">
                                <div class="input-group">
                                    <label for="ls_add_finalSettle_attachment_Others" class="input-group-text">
                                        <b>Other</b> &nbsp; Supporting Documents Upload (if applicable)
                                    </label>
                                    <input type="file" id="ls_add_finalSettle_attachment_Others" name="ls_add_finalSettle_attachment_Others" class="form-control" multiple />
                                </div>
                                <small class="text-muted">Accepted file types: .jpeg, .pdf, .doc etc</small>
                                <div class="mt-2 uploaded-design-others"></div>
                            </div>
                        </div>
                    </div>

                    <div class="modal-footer">
                        <button type="button" class="btn btn-light-secondary rounded-pill" data-bs-dismiss="modal">
                            <i class="bx bx-x d-block d-sm-none"></i><span class="d-none d-sm-block">Close</span>
                        </button>
                        <button type="submit" class="btn btn-primary rounded-pill ml-1">
                            <i class="bx bx-check d-block d-sm-none"></i>
                            <span class="d-none d-sm-block">Save</span>
                        </button>
                    </div>

                </form>
            </div>
        </div>
    </div>




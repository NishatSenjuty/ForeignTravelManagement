
@{
    ViewBag.Title = "Final Settlement";
}
@section styles{


    <style>
        .ui-datepicker {
            z-index: 9999 !important;
        }

        div.ui-datepicker {
            font-size: 10px;
        }

        #applicant-details-table {
            border-collapse: collapse;
            border: 1px solid #ddd;
        }

            #applicant-details-table th, #applicant-details-table td {
                border: 1px solid #ddd;
                padding: 8px;
                text-align: left;
            }

            #applicant-details-table th {
                background-color: #f2f2f2;
            }

            #applicant-details-table center {
                display: block;
            }

        th {
            text-align: center;
        }

        .disabled {
            pointer-events: none;
            opacity: 0.5;
        }

    </style>




}
<div class="page-heading">
    <section class="section">
        <div class="section-body">
            <div class="row">
                <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xs-12">
                    <div class="card  shadow">
                        <div class="card-header" style="background-color:#e0e0e0">
                            <div class="row">
                                <div class="col-12 col-md-6 order-md-1 order-last">
                                    <h3>Final Settlement</h3>
                                </div>
                            </div>
                        </div>
                        <div class="card-body">

                            <form class="form-inline" method="get" id="gatepass_filter_form" name="gatepass_filter_form">

                                <div class="row mt-3">

                                    <div class="col-lg-3 mb-1">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text">
                                                Budget Approval No.
                                            </span>
                                            <input id="filter_disburse_no" name="filter_disburse_no" class="form-control" />
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Company</span>
                                            <select class="form-control form-control form-select" id="ls_filter_rp_EmpInfo_Company" name="ls_filter_rp_EmpInfo_Company">
                                                <option value="">Select a Company</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Department</span>
                                            <select class="form-control form-control form-select" id="ls_filter_rp_EmpInfo_department" name="ls_filter_rp_EmpInfo_department">
                                                <option value="">Select a Department</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-text">Designation</span>
                                            <select class="form-control form-control form-select" id="ls_filter_rp_EmpInfo_designation" name="ls_filter_rp_EmpInfo_designation">
                                                <option value="">Select a Designation</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-8"></div>

                                        <div class="col-lg-3">
                                            <div class="btn-group">
                                                <button type="button" class="btn btn-soft-blue" id="ls_filter_btn" name="ls_filter_btn">Filter</button>
                                            </div>
                                        </div>
                                    </div>
                            </form>


                            <br /><br />

                            <div class="table-responsive mt-2">
                                <table class="table table-striped table-hover" id="ls_finalSet_table">
                                    <thead>
                                        <tr>
                                            <th>SL NO</th>
                                            <th> Budget Approval NO</th>
                                            <th>Employee ID NO</th>
                                            <th>Name</th>
                                            <th>Contact NO</th>
                                            <th>Company</th>
                                            <th>Department</th>
                                            <th>Designation</th>
                                            <th>Estimated Cost</th>
                                            <th>Disbursed amount</th>
                                            <th>Approve Status By Supervisor</th>
                                            <th>Comments (If rejected By Supervisor)</th>
                                            <th>Approve Status By Audit</th>
                                            <th>Remarks (Audit)</th>
                                            <th>Approve Status By Management</th>
                                            <th>Comments (If rejected By Management)</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody id="ls_finalSet_table_tbody"></tbody>
                                </table>
                            </div>



                            <div class="row">
                                @*table info*@
                                <div class="col-md-4">
                                    <span class="badge bg-primary" id="ls_finalSet_table_info"></span>
                                </div>
                                @*table spinner*@

                                <div class="col-md-4">
                                    <div class="text-center" id="spinner">
                                        <img src="~/Content/assets/vendors/svg-loaders/rings.svg" class="" style="width: 3rem" alt="audio"><span style="font-weight: 700">Loading...</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!--finalSet add modal -->

    <div class="modal fade bd-example-modal" id="ls_add_finalSet_modal">
        <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-header bg-primary">
                    <h5 class="modal-title white">
                        Final Settlement
                    </h5>
                    <button type="button" class="close text-white"
                            data-bs-dismiss="modal" aria-label="Close">
                        <i data-feather="x"></i>
                    </button>
                </div>
                <form id="ls_add_finalSet_modal_form" name="ls_add_finalSet_modal_form" class="needs-validation" novalidate>

                    <div class="modal-body" style="max-height:75vh;overflow:auto">

                        <table id="applicant-details-table" style="width: 100%; margin: 0 auto; margin-top: 10px;">
                            <tr>
                                <th colspan="12"><center>Applicant Details</center></th>
                            </tr>
                            <tr>
                                <td>Employee Name </td>
                                <td id="applicant_name"></td>
                                <td>Employee ID </td>
                                <td id="applicant_employee_id"></td>
                                <td>Company</td>
                                <td id="applicant_company"></td>
                                <td>Department</td>
                                <td id="applicant_department"></td>
                                <td>Designation</td>
                                <td id="applicant_designation"></td>
                            </tr>
                        </table>

                        <br />

                        <!-- Team Member Details -->
                        <div class="card border-primary mb-3">
                            <div class="card-header">
                                <h4 class="card-title">Visa, Ticket and Hotel Reservation Requisitions Details</h4>
                            </div>

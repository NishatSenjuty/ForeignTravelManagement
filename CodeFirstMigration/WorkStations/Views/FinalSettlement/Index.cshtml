                                <div class="table-responsive">
                                    <table class="table table-striped table-hover" id="ls_team_table_edit">
                                        <thead class="table-primary">
                                            <tr>
                                                <th>Visa Requisition No.</th>
                                                <th>Ticket Requisition No.</th>
                                                <th>Hotel Requisition No.</th>
                                            </tr>
                                        </thead>
                                        <tbody id="ls_team_table_edit_tbody">
                                            <tr id="ginnuepig_edit" class="text-center add-item">
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                        </tbody>


                                    </table>
                                </div>
                            </div>
                        </div>

                        <div class="alert alert-info" role="alert">
                            <strong>Notice:</strong> The food allowance of $100 USD has been established. If you are invoicing in a different currency, please convert and submit the equivalent amount to $100 USD.
                        </div>


                        <div class="row">
                            <div class="card" style="border: 1px solid #25396f;">
                                <div class="card-header">
                                    <h4 class="card-title">Travel Cost</h4>
                                </div>
                                <div class="card-body" style="padding: 0rem 1rem 1rem 1rem;">
                                    <div class="table-responsive">
                                        <table class="table table-striped table-hover" id="ord_order_table">
                                            <thead>
                                                <tr>
                                                    <th style="width: 150px">Expenses</th>
                                                    <th style="width: 150px">Days</th>
                                                    <th style="width: 150px">Cost per Day</th>
                                                    <th style="width: 150px">Total</th>
                                                </tr>
                                            </thead>
                                            <tbody id="ord_order_table_tbody">
                                                <tr class="text-center add-item">
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">

                            @*Budget Unique No.*@
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-text"> Budget Unique No.<span style="color: red;">*</span></span>
                                    <input id="ls_add_finalSet_budgetno" name="ls_add_finalSet_budgetno" class="form-control" required readonly />
                                </div>
                            </div>

                            @*Estimated Cost*@
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-text">
                                        Estimated Cost<span style="color: red;">*</span>
                                    </span>
                                    <input id="ls_add_finalSet_estimated_cost" name="ls_add_finalSet_estimated_cost" class="form-control" required readonly />
                                </div>
                            </div>

                            @*Disbursed Amount*@


@{
    ViewBag.Title = "Employee Information";
}
@section styles{



    <style>

        /*.datepicker {
            z-index: 2051 !important;
        }*/
        .ui-datepicker {
            z-index: 9999 !important;
        }
    </style>
}
<div class="page-heading">
    <section class="section">
        <div class="section-body">
            <div class="row">
                <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xs-12">
                    <div class="card  shadow">
                        <div class="card-header" style="background-color:#e0e0e0">
                            <div class="row">
                                <div class="col-12 col-md-6 order-md-1 order-last">
                                    <h3>Employee Information</h3>
                                </div>
                                <div class="col-12 col-md-6 order-md-2 order-first">
                                    <div class="breadcrumb-header float-start float-lg-end">
                                        <a  class="btn btn-primary rounded-pill" data-toggle="modal" id="ls_add_EmpInfo_btn">Add Employee</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card-body">


                            <!--EmpInfo filter section start-->


                            <form class="form-inline" method="get" id="ls_rp_EmpInfo_filter_form" name="ls_rp_EmpInfo_filter_form">

                                <div class="container">
                                    <div class="row">

                                        <div class="col-lg-3">
                                            <div class="input-group">
                                                <span class="input-group-text">Limit</span>
                                                <select id="ls_filter_limit" name="ls_filter_limit" class="form-control form-select">
                                                    <option value="10" selected>10</option>
                                                    <option value="25">25</option>
                                                    <option value="50">50</option>
                                                    <option value="100">100</option>
                                                    <option value="250">250</option>
                                                </select>
                                            </div>
                                        </div>


                                        <div class="col-lg-3">
                                            <div class="input-group">
                                                <span class="input-group-text">Employee ID</span>
                                                <input id="ls_rp_EmpInfo_filter_Id" name="ls_rp_EmpInfo_filter_Id" class="form-control" />
                                            </div>
                                        </div>

                                        <div class="col-lg-3">
                                            <div class="input-group">
                                                <span class="input-group-text">Name</span>
                                                <input id="ls_rp_EmpInfo_filter_Name" name="ls_rp_EmpInfo_filter_Name" class="form-control" />
                                            </div>
                                        </div>

                                        <div class="col-lg-3">
                                            <div class="input-group">
                                                <span class="input-group-text">Company</span>
                                                <input id="ls_filter_rp_EmpInfo_Company" name="ls_filter_rp_EmpInfo_Company" class="form-control" />
                                            </div>
                                        </div>

                                        <div class="col-lg-3">
                                            <div class="input-group">
                                                <span class="input-group-text">Department</span>
                                                <input id="ls_filter_rp_EmpInfo_department" name="ls_filter_rp_EmpInfo_department" class="form-control" />
                                            </div>
                                        </div>

                                        <div class="col-lg-3">
                                            <div class="input-group">
                                                <span class="input-group-text">Designation</span>
                                                <input id="ls_filter_rp_EmpInfo_designation" name="ls_filter_rp_EmpInfo_designation" class="form-control" />
                                            </div>
                                        </div>

                                        <div class="col-lg-4">
                                            <div class="btn-group">
                                                <button type="button" class="btn btn-soft-blue" id="ls_filter_btn" name="ls_filter_btn">Filter</button>
                                                <button type="button" class="btn btn-soft-success" id="ls_print_btn" name="ls_print_btn">Print</button>
                                                <button type="button" class="btn btn-soft-info" onclick="tableToExcel(ls_EmpInfo_table)" id="ls_excel_btn" name="ls_excel_btn">Excel Download</button>
                                            </div>                                            
                                        </div>
                                    </div>

                                </div>

                            </form>

                            <!--EmpInfo filter section end-->
                            <div class="table-responsive mt-2">
                                <table class="table table-striped table-hover" id="ls_EmpInfo_table">
                                    <thead>
                                        <tr>
                                            <th>SL NO</th>
                                            <th>Employee ID NO</th>
                                            <th>Name</th>
                                            <th>Company</th>
                                            <th>Location</th>
                                            <th>Department</th>
                                            <th>Designation</th>
                                            <th>Contact NO</th>
                                            <th>Employee Status</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody id="ls_EmpInfo_table_tbody"></tbody>

                                </table>
                            </div>

                            <div class="row">
                                @*table info*@
                                <div class="col-md-4">
                                    <span class="badge bg-primary" id="ls_EmpInfo_table_info"></span>
                                </div>
                                @*table spinner*@

                                <div class="col-md-4">
                                    <div class="text-center" id="spinner">
                                        <img src="~/Content/assets/vendors/svg-loaders/rings.svg" class="" style="width: 3rem" alt="audio"><span style="font-weight: 700">Loading...</span>
                                    </div>
                                </div>

                                @*table pagination*@
                                <div class="col-md-4">
                                        <nav aria-label="Page navigation example">
                                            <ul class="pagination pagination-primary  justify-content-end">
                                                <li class="page-item">
                                                    <a class="page-link" id="ls_EmpInfo_previous_list_click" title="Previous">
                                                        <span aria-hidden="true"><i class="bi bi-chevron-left"></i></span>
                                                    </a>
                                                </li>
                                                <li class="page-item active">
                                                    <a class="page-link" id="ls_EmpInfo_pagination_page_no" title="Click here for jump to page">1</a>
                                                </li>
                                                <li class="page-item">
                                                    <input id="ls_EmpInfo_pagination_page_no_input" class="page-link display-none" style="width:50px" />
                                                </li>
                                                <li class="page-item">
                                                    <a class="page-link" id="ls_EmpInfo_next_list_click" title="Next">
                                                        <span aria-hidden="true"><i class="bi bi-chevron-right"></i></span>
                                                    </a>
                                                </li>
                                            </ul>
                                        </nav>
                                    </div>

                            </div>


                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!--EmpInfo add modal -->
    <div class="modal fade bd-example-modal" id="ls_add_EmpInfo_modal">
        <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header bg-primary">
                    <h5 class="modal-title white">
                        Add Employee
                    </h5>
                    <button type="button" class="close text-white"
                            data-bs-dismiss="modal" aria-label="Close">
                        <i data-feather="x"></i>
                    </button>
                </div>
                <form id="ls_add_EmpInfo_modal_form" name="ls_add_EmpInfo_modal_form" class="needs-validation" novalidate>
                    <div class="modal-body" style="max-height:75vh;overflow:auto">
                        <div class="row">
                            <div class="col-sm-6">

                                @*First Column Starts*@

                                @*ID*@
                                <div class="form-group">
                                    @*<label for="ls_add_EmpInfo_id"><span class="text-danger">*</span>Employee ID</label>
                                        <input type="text" class="form-control" id="ls_add_EmpInfo_id" name="ls_add_EmpInfo_id" placeholder="Enter Employee ID" required>*@
                                    <div class="input-group">
                                        <span class="input-group-text">Employee ID</span>
                                        <input id="ls_add_EmpInfo_id" name="ls_add_EmpInfo_id" class="form-control" @*placeholder="Enter Employee ID"*@ required />
                                    </div>
                                    <div class="invalid-feedback">
                                        Please enter Employee ID.
                                    </div>
                                </div>

                                @*Name*@
                                <div class="form-group">
                                    @*<label for="ls_add_EmpInfo_name"><span class="text-danger">*</span>Employee ID</label>*@
                                    @*<input type="text" class="form-control" id="ls_add_EmpInfo_name" name="ls_add_EmpInfo_name" placeholder="Enter Employee Name" required>*@

                                    <div class="input-group">
                                        <span class="input-group-text"> Name</span>
                                        <input id="ls_add_EmpInfo_name" name="ls_add_EmpInfo_name" class="form-control" placeholder="Enter Employee Name" required />
                                    </div>
                                    <div class="invalid-feedback">
                                        Please enter Employee Name.
                                    </div>
                                </div>

                                @*Join Date*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">Join Date</span>
                                        <input type="text" id="ls_add_EmpInfo_JoinDate" name="ls_add_EmpInfo_JoinDate" class="form-control" @*placeholder="Enter Join Date"*@ required />
                                    </div>
                                    <div class="invalid-feedback">
                                        Please Enter join date.
                                    </div>
                                </div>

                                @*Date Of Birth*@
                                <div class="input-group">
                                    <span class="input-group-text">Date Of Birth</span>
                                    <input type="text" id="ls_add_EmpInfo_dob" name="ls_add_EmpInfo_dob" class="form-control" @*placeholder="Enter Join Date"*@ required />
                                </div>
                                <div class="invalid-feedback">
                                    Please Enter Date Of Birth.
                                </div>

                                @*dropdown, Gender*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">Gender</span>
                                        <select id="ls_add_EmpInfo_gender" name="ls_add_EmpInfo_gender" class="form-control form-select">
                                            <option value="-1">Select Gender...</option>
                                            <option value="1">Male</option>
                                            <option value="2">Female</option>
                                            <option value="3">Others</option>
                                        </select>
                                    </div>
                                    <div class="invalid-feedback">
                                        Please select Gender.
                                    </div>
                                </div>

                                @*dropdown, data from company table*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">Company/Branch</span>
                                        <select id="ls_add_EmpInfo_company" name="ls_add_EmpInfo_company" class="form-control form-select">
                                            <option value="-1">Select Company...</option>
                                        </select>
                                    </div>
                                    <div class="invalid-feedback">
                                        Please select Company name.
                                    </div>
                                </div>

                                @*dropdown, data from location table*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">Location</span>
                                        <select id="ls_add_EmpInfo_location" name="ls_add_EmpInfo_location" class="form-control form-select">
                                            <option value="-1">Select Location...</option>
                                        </select>
                                    </div>
                                    <div class="invalid-feedback">
                                        Please select Location name.
                                    </div>
                                </div>


                            </div>
                            @*First Column Ends*@



                            @*Second Column Starts*@

                            <div class="col-sm-6">

                                @*Contact Address*@
                                <div class="form-group">
                                    @*<label for="ls_add_EmpInfo_address">Contact Address</label>*@
                                    @*<input type="text" class="form-control" id="ls_add_EmpInfo_address" name="ls_add_EmpInfo_address" placeholder="Enter Employee Contact Address" required>*@
                                    <div class="input-group">
                                        <span class="input-group-text"> Address</span>
                                        <input id="ls_add_EmpInfo_address" name="ls_add_EmpInfo_address" class="form-control" @*placeholder="Enter Employee ID"*@ required />
                                    </div>
                                    <div class="invalid-feedback">
                                        Please enter Employee Contact Address.
                                    </div>
                                </div>



                                @*dropdown, data from department table*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">Department</span>
                                        <select id="ls_add_EmpInfo_department" name="ls_add_EmpInfo_department" class="form-control form-select">
                                            <option value="-1">Select Department...</option>
                                        </select>
                                    </div>
                                    <div class="invalid-feedback">
                                        Please select Department name.
                                    </div>
                                </div>

                                @*dropdown, data from designation table*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">Designation</span>
                                        <select id="ls_add_EmpInfo_designation" name="ls_add_EmpInfo_designation" class="form-control form-select">
                                            <option value="-1">Select Designation...</option>
                                        </select>
                                    </div>
                                    <div class="invalid-feedback">
                                        Please select Designation name.
                                    </div>
                                </div>

                                @*Blood Group*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">Blood Group</span>
                                        <select id="ls_add_EmpInfo_BloodGroup" name="ls_add_EmpInfo_BloodGroup" class="form-control form-select">
                                            <option value="-1">Select Blood Group...</option>
                                            <option value="1">O+</option>
                                            <option value="2">A+</option>
                                            <option value="3">B+</option>
                                            <option value="4">AB+</option>
                                            <option value="5">A-</option>
                                            <option value="6">B-</option>
                                            <option value="7">AB-</option>
                                            <option value="8">O-</option>
                                        </select>
                                    </div>
                                    <div class="invalid-feedback">
                                        Please select Blood Group.
                                    </div>
                                </div>

                                @*Salary*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">Salary</span>
                                        <input id="ls_add_EmpInfo_salary" name="ls_add_EmpInfo_salary" class="form-control" required />
                                    </div>
                                    <div class="invalid-feedback">
                                        Please enter Salary.
                                    </div>
                                </div>

                                @*Contact No*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">Contact No</span>
                                        <input id="ls_add_EmpInfo_ContactNo" name="ls_add_EmpInfo_ContactNo" class="form-control" required />
                                    </div>
                                    <div class="invalid-feedback">
                                        Please Enter Contact No.
                                    </div>
                                </div>

                                @*Active Status*@
                                <div class="form-group">
                                    <div class="input-group">
                                        <span class="input-group-text">Employee Status</span>
                                        <select id="ls_add_EmpInfo_status" name="ls_add_EmpInfo_status" class="form-control form-select">
                                            <option value="-1">Select Employee Status...</option>
                                            <option value="1" selected>Active</option>
                                            <option value="0">InActive</option>
                                        </select>
                                    </div>
                                    <div class="invalid-feedback">
                                        Please select Employee Status.
                                    </div>
                                </div>

                            </div>
                            @*Second Column Ends*@
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-light-secondary rounded-pill" data-bs-dismiss="modal">
                            <i class="bx bx-x d-block d-sm-none"></i><span class="d-none d-sm-block">Close</span>
                        </button>
                        <button type="submit" class="btn btn-primary rounded-pill ml-1">
                            <i class="bx bx-check d-block d-sm-none"></i>
                            <span class="d-none d-sm-block">Save</span>
                        </button>
                    </div>
                </form>

            </div>
        </div>
    </div>
    @*Employee Add Modal*@





    <!--EmpInfo edit modal -->
